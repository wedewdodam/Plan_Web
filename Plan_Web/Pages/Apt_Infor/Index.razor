@page "/Apt_Infor"

@if (bnn == null)
{
    <p style="text-align:center">
        <img src="/images/loader.gif" height="100" />
    </p>
}
else
{
    <div class="card">
        <div class="card-header">
            <table width="100%">
                <tr>
                    <td><span style="font-size: 1em"> <span class="bi-alarm"></span>&nbsp;<span>단지 개요</span></span></td>
                    <td align="right">
                        <button type="button" class="btn btn-outline-secondary btn-sm" @onclick="btnOpen">@strTitle</button>
                    </td>
                </tr>
            </table>
        </div>
        <div class="card-body">
            <table class="table" style="font-size: 0.9em; margin-bottom: 0px">
                <tr style="border-top:solid; border-top-width:2px; border-top-color: cadetblue">
                    <td align="center" style="background-color:whitesmoke" width="10%">
                        시행사
                    </td>
                    <td width="23%">@bnn.Developer</td>
                    <td align="center" style="background-color:whitesmoke" width="10%">
                        시공사
                    </td>
                    <td width="23%">@bnn.Builder</td>
                    <td align="center" style="background-color:whitesmoke" width="10%">
                        지역·지구
                    </td>
                    <td>@bnn.District</td>
                </tr>
                <tr>
                    <td align="center" style="background-color:whitesmoke">
                        관리방식
                    </td>
                    <td>@bnn.Management_Way</td>
                    <td align="center" style="background-color:whitesmoke">
                        승강기 수
                    </td>
                    <td>@bnn.Elevator</td>
                    <td align="center" style="background-color:whitesmoke">
                        관리형식
                    </td>
                    <td>@bnn.Joint_Management</td>
                </tr>
                <tr>
                    <td align="center" style="background-color:whitesmoke">
                        대지면적(㎡)
                    </td>
                    <td>@string.Format("{0: ###,###.####}", bnn.Site_Area)<span>㎡</span></td>
                    <td align="center" style="background-color:whitesmoke">
                        건축면적(㎡)
                    </td>
                    <td>@string.Format("{0: ###,###.####}", bnn.Build_Area)<span>㎡</span></td>
                    <td align="center" style="background-color:whitesmoke">
                        연면적(㎡)
                    </td>
                    <td>@string.Format("{0: ###,###.####}", bnn.FloorTotal_Area)<span>㎡</span></td>
                </tr>
                <tr>
                    <td align="center" style="background-color:whitesmoke">
                        공급면적(㎡)
                    </td>
                    <td>@string.Format("{0: ###,###.####}", bnn.Supply_Area)<span>㎡</span></td>
                    <td align="center" style="background-color:whitesmoke">
                        용적률(%)
                    </td>
                    <td>@string.Format("{0: ###,###.####}", bnn.FloorArea_Ratio)<span>%</span></td>
                    <td align="center" style="background-color:whitesmoke">
                        건폐률(%)
                    </td>
                    <td>@string.Format("{0: ###,###.####}", bnn.BuildingCoverage_Ratio)<span>%</span></td>
                </tr>
                <tr>
                    <td align="center" style="background-color:whitesmoke">
                        수전용량
                    </td>
                    <td>@string.Format("{0: ###,###.####}", bnn.Electric_Supply_Capacity)<span>KVA</span></td>
                    <td align="center" style="background-color:whitesmoke">
                        저수량(톤)
                    </td>
                    <td>@string.Format("{0: ###,###.####}", bnn.Water_Quantity)<span>톤</span></td>
                    <td align="center" style="background-color:whitesmoke">
                        주차대수
                    </td>
                    <td>@string.Format("{0: ###,###.####}", bnn.Park_Car_Count)<span>대</span></td>
